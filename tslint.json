{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            true,
            111
        ],
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [ false ]
    },
    "rulesDirectory": []
}
